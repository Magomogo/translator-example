{
    "name": "Magomogo/translator-example",
    "description": "example to be translated with Translator application",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "hamcrest/hamcrest",
                "version": "0.3.0",
                "source": {
                    "url": "http://hamcrest.googlecode.com/svn/",
                    "type": "svn",
                    "reference": "tags/hamcrest-php-0.3.0/"
                },
                "autoload": {
                    "psr-0": {
                        "Hamcrest": "hamcrest/"
                    },
                    "files": ["hamcrest.php"]
                }
            }
        },
        {
            "type": "vcs",
            "url": "https://github.com/Magomogo/translator-utils"
        }
    ],
    "require": {
        "Magomogo/translator-utils": "*"
    },
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Maxim Gnatenko",
            "email": "maxim@xiag.ch"
        }
    ]
}
